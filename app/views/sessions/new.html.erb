<h1>Sign in page</h1>


<%= form_with url: signin_create_path do |form| %>
    <div class="mt-3">
        <%= form.label :email%>
        <%= form.email_field :email, class: "form-control", placeholder: "email@email.com"%>
        <div>

    <div class="mt-3">
        <%= form.label :password%>
        <%= form.password_field :password, class: "form-control", placeholder: "Password"%>
    <div>

    <div class="mt-3">
        <%= form.submit "sign up here", class: "btn btn-primary"%>
    <div>

<% end %>